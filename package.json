    "prettier": "2.6.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "reset-all": "npm cache clean --force && rm -rf node_modules && rm yarn.lock && yarn install --network-concurrency 1",
    "lint": "eslint . -c .eslintrc.json --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . -c .eslintrc.json --ext js,jsx,ts,tsx --quiet --fix",
    "format": "prettier-eslint --eslint-config-path ./.eslintrc.json --write '**/*.tsx'"

  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
